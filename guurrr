class Aves3(chordata2):#class
    def __init__(self, name):
        super().__init__(name)
        self.species = 'They have feathers with toothless beaked jaws. Also they lay hard-shelled eggs.'

    def display_properties(self):
        super().display_properties()
        print("Name of taxonomy rank is:", self.name)
        print("The facts about this rank is:", self.species)

class Galliformes4(Aves3): #order turkey,chicken
    def __init__(self, name):
        super().__init__(name)
        self.species = 'They are heavybodied compared to other species under the Aves classification. They are needed since they spread seeds throughout the ecosystem. They are known for running away from predators rather than flying away.'

    def display_properties(self):
        super().display_properties()
        print("Name of taxonomy rank is:", self.name)
        print("The facts about this rank is:", self.species)

class Phasianidae5(Galliformes4):#family
    def __init__(self, name):
        super().__init__(name)
        self.species = 'This family is known consisting majority of the gamebirds in the world. Phasiandaes are ground-living birds rather than in nests '

    def display_properties(self):
        super().display_properties()
        print("Name of taxonomy rank is:", self.name)
        print("The facts about this rank is:", self.species)

class Meleagris6(Phasianidae5):#genus v
    def __init__(self, name):
        super().__init__(name)
        self.species = 'Males of this genus have a wattle that is connected at the bottom of the beak. They are the largest birds in their classifications.'

    def display_properties(self):
        super().display_properties()
        print("Name of taxonomy rank is:", self.name)
        print("The facts about this rank is:", self.species)

class Meleagris_gallopavo7(Meleagris6):# specis wild turkey
    def __init__(self, name):
        super().__init__(name)
        self.species = 'Wild turkeys are upland game bird which is the heaviest compared to the other species.  It is the ancestor to the domestic turkey'

    def display_properties(self):
        super().display_properties()
        print("Name of taxonomy rank is:", self.name)
        print("The facts about this rank is:", self.species)

