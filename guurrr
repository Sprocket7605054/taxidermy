    def display_properties(self):
        super().display_properties()
        print("Name of taxonomy rank is:", self.name)
        print("The facts about this rank is:", self.species)

class 	Gallus6(Phasianidae5):
    def __init__(self, name):
        super().__init__(name)
        self.species = 'The jungle fowl has four species under it. some of the species is the multitude of junglefowls and chicken. Jungle fowls are omnivores. They usally live in a social class that are agressive to each other where the term pecking order comes form.'

    def display_properties(self):
        super().display_properties()
        print("Name of taxonomy rank is:", self.name)
        print("The facts about this rank is:", self.species)

class Gallus_gallus_domesticus7(Gallus6):
    def __init__(self, name):
        super().__init__(name)
        self.species = 'The chicken is a large bird compared to  junglefowls. They also have short wings.'

    def display_properties(self):
        super().display_properties()
        print("Name of taxonomy rank is:", self.name)
        print("The facts about this rank is:", self.species)

class Gallus_gallus7(Gallus6):
    def __init__(self, name):
        super().__init__(name)
        self.species = 'The color of the feather ranges from red, green,orange,gold.'

    def display_properties(self):
        super().display_properties()
        print("Name of taxonomy rank is:", self.name)
        print("The facts about this rank is:", self.species)

class Gallus_lafayettii7(Gallus6):
    def __init__(self, name):
        super().__init__(name)
        self.species = 'Sir Lakan Junglefowl is closly related to grey junglefowl. The males often resemble the red junglefowl while the females reseamble the grey junglefowl.'

    def display_properties(self):
        super().display_properties()
        print("Name of taxonomy rank is:", self.name)
        print("The facts about this rank is:", self.species)


# Creating an instance of Animalia1
animalia_instance = Animalia1("Kingdom dog")
chordata_instance = chordata2("Phylum Dogs")
